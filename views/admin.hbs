{{> navbar user=user}}
<html>
  <head>
    <title>{{title}}</title>
  </head>
  <body>
    <div class="container">
    
    <h1>{{title}}</h1>

    <h2>Users</h2>
    <table border="1" cellpadding="4">
      <tr><th>UserID</th><th>Role</th></tr>
      {{#each users}}
        <tr><td>{{userid}}</td><td>{{role}}</td></tr>
      {{/each}}
    </table>

    <h2 style="margin-top:2em">Results</h2>
    <table border="1" cellpadding="4">
      <tr>
        <th>When</th><th>Player</th><th>Game</th><th># Correct</th>
      </tr>
      {{#each results}}
        <tr>
          <td>{{result_time}}</td>
          <td>{{player}}</td>
          <td>{{game}}</td>
          <td>{{score}}</td>
        </tr>
      {{/each}}
    </table>

    <p><a href="/games">â¬… back to games</a></p>
  </body></div>
</html>